<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nefu.myspringboot.mapper.StudentMapper">

    <select id="list" resultType="com.nefu.myspringboot.dto.LaboratoryDTO">
        select lc.lab_id,
               c.number,c.teacher_id,c.name,
               from lab_coures lc join user course c on lc.id=c.id
    </select>
</mapper>